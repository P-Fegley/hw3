<!-- Place details page -->

<h1><%= @place["name"] %> Travel Log</h1>

<% for entry in @entries %>
  <p>---</p>
  <h2><%= entry["title"] %></h2>
  <p><%= entry["occurred_on"] %></p>
  <p><%= entry["description"] %></p>
  <p> </p>
<% end %>

<p>
  <a href="/entries/new?place_id=<%= @place["id"]%>">New Post</a>
</p>

<p>
  <a href="/places">Back to Travel Log</a>
</p>